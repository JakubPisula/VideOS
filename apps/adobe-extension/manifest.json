{
  "id": "com.creative.secondbrain.premiere",
  "name": "Creative Second Brain",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PPRO",
      "minVersion": "22.0"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "mainContext",
      "label": {
        "default": "CSB Workflow"
      },
      "minimumSize": { "width": 300, "height": 400 },
      "maximumSize": { "width": 800, "height": 1200 }
    }
  ],
  "permissions": {
    "network": {
      "domains": ["https://api.notion.com"]
    },
    "localFileSystem": "request"
  }
}
